{
  "name": "babel-plugin-array-last-index",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/lijinke666/babel-plugin-array-last-index.git"
  },
  "scripts": {
    "test": "jest"
  },
  "files": [
    "src"
  ],
  "main": "src/index.js",
  "author": "Jinke.Li",
  "license": "MIT",
  "keywords": [
    "module",
    "npm",
    "babel-plugin-array-last-index"
  ],
  "pre-commit": "lint",
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint_d --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "/.history/",
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__tests__/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/types": "^7.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/preset-env": "^7.4.3",
    "babel-jest": "^24.7.1",
    "jest": "^24.7.1"
  }
}
